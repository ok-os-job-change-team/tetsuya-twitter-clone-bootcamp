# -*- mode: ruby -*-
# vi: set ft=ruby :
# ridgepole導入とridgepole.rakeにより、コマンドrake ridgepole:apply testでschema.rbとrails_test.usersに反映される
create_table 'users', charset: 'utf8mb4', collation: 'utf8mb4_bin', comment: 'ユーザー', force: :cascade do |t|
  t.string 'email', default: '', null:false, comment: 'メールアドレス'
  t.string 'password', null: false, comment: 'パスワード'
  t.timestamp 'created_at', null: false, comment: '作成日時'
  t.timestamp 'updated_at', null: false, comment: '更新日時'
end